{
  "name": "multi-nprogress",
  "author": "Ramy Rais",
  "contributors": [
    {
      "name": "Rico Sta. Cruz",
      "email": "hi@ricostacruz.com"
    }
  ],
  "description": "Simple slim progress bars",
  "version": "0.3.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/RamyRais/multi-nprogress"
  },
  "scripts": {
    "test": "mocha"
  },
  "main": "nprogress.js",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.1.2",
    "jquery": "^3.5.1",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "mocha": "^7.1.2"
  },
  "dependencies": {},
  "jspm": {
    "format": "global",
    "shim": {
      "nprogress": {
        "deps": [
          "./nprogress.css!"
        ]
      }
    },
    "dependencies": {
      "css": "*"
    }
  },
  "spm": {
    "main": "nprogress.js",
    "output": [
      "nprogress.css"
    ],
    "ignore": [
      "support",
      "test",
      "vendor"
    ]
