{
  "chatAuthentication": {
    "schema": {
      "method": "GET",
      "host": "<%= context.freshchat_domain %>",
      "path": "/v2/accounts/configuration",
      "headers": {
        "Authorization": "Bearer <%= context.freshchat_token %>",
        "Content-Type": "application/json"
      }
    }
  },
  "getAgentDetails": {
    "schema": {
      "method": "GET",
      "host": "<%= context.freshchat_domain %>",
      "path": "/v2/agents?items_per_page=100",
      "headers": {
        "Authorization": "Bearer <%= context.freshchat_token %>",
        "Content-Type": "application/json"
      }
    }
  },
  "getGroupDetails": {
    "schema": {
      "method": "GET",
      "host": "<%= context.freshchat_domain %>",
      "path": "/v2/groups?items_per_page=100",
